{
  "extends": "../../tsconfig.settings.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "rootDir": "./src",
    "paths": {
      "common": ["../common"]
    },
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "references": [{ "path": "../common" }],
  "include": ["src", "src/**/*.json", "ApiError.ts"],
  "exclude": ["./dist", "./node_modules"]
}
